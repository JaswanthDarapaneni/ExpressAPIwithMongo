{
    "version": 2,
    "name": "api",
    "builds": [
      { "src": "src/app.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "src/app.js" }
    ],
    "functions": {
        "api": {
          "runtime": "nodejs14.x",
          "memory": 1024,
          "includeFiles": "src/app.js"
        }
      }
    
  }
  